{
  "name": "manger-http",
  "version": "3.0.3",
  "description": "HTTP/1.1 API for caching RSS feeds",
  "main": "index.js",
  "scripts": {
    "pretest": "standard",
    "test": "tap -b test/*.js",
    "posttest": "rm -rf /tmp/manger-http-[1-9]*",
    "start": "MANGER_LOG_LEVEL=30 node start.js | node_modules/bunyan/bin/bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/manger-http"
  },
  "keywords": [
    "feed",
    "cache",
    "http",
    "server"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelnisi/manger-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/manger-http",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bunyan": "^1.8.5",
    "http-hash": "^2.0.0",
    "http-methods": "^1.0.0",
    "manger": "^6.1.3",
    "mkdirp": "^0.5.1",
    "negotiator": "^0.6.1"
  },
  "devDependencies": {
    "rimraf": "^2.5.4",
    "standard": "^10.0.3",
    "tap": "^11.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "author": "Michael Nisi <michael.nisi@gmail.com> (http://troubled.pro/)"
}
